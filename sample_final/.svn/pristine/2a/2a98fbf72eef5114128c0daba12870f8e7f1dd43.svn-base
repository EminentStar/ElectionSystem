package net.javabugs.web.example.model.dto;

public class Comment {
	private String cmtNum; 
	
	private String number_bid; 
	
	private String comments;
	
	private String cid;
	
	private String crtDttm;
	
	private String crtCid;
	
	private String updDttm;
	
	private String updCid;

	public String getCmtNum() {
		return cmtNum;
	}

	public void setCmtNum(String cmtNum) {
		this.cmtNum = cmtNum;
	}

	

	public String getNumber_bid() {
		return number_bid;
	}

	public void setNumber_bid(String number_bid) {
		this.number_bid = number_bid;
	}

	public String getComments() {
		return comments;
	}

	public void setComments(String comments) {
		this.comments = comments;
	}

	public String getCid() {
		return cid;
	}

	public void setCid(String cid) {
		this.cid = cid;
	}

	public String getCrtDttm() {
		return crtDttm;
	}

	public void setCrtDttm(String crtDttm) {
		this.crtDttm = crtDttm;
	}

	public String getCrtCid() {
		return crtCid;
	}

	public void setCrtCid(String crtCid) {
		this.crtCid = crtCid;
	}

	public String getUpdDttm() {
		return updDttm;
	}

	public void setUpdDttm(String updDttm) {
		this.updDttm = updDttm;
	}

	public String getUpdCid() {
		return updCid;
	}

	public void setUpdCid(String updCid) {
		this.updCid = updCid;
	}
	
	
	
}
