@charset "utf-8";

.elected_person {
	height: 1600px;
	width: 778px;




html, body {
	margin: 0;
	padding: 0;
	width: 100%;
	height: 100%; 
	font-family: "맑은 고딕";
}

.content {
	min-height: 100%;
	position: relative;
	overflow: auto;
	z-index: 0;
	font-family: "맑은 고딕";
	font-size: 12px;
	font-weight: bold;
	width: 100%;

}

.background {
	position: absolute;
	z-index: -1;
	top: 0;
	bottom: 0;
	margin: 0;
	padding: 0;
	background-color: #FFF;
	width: 100%;
}

.top_block {
	width: 100%;
	display: block;
	border: 0.3px double #CCC;
}


.bottom_block {
	position: absolute;
	width: 100%;
	display: block;
	bottom: 0;
	border: 0.5px double #CCC;
}

.left_block {
	display: block;
	float: left; 
}

.right_block {
	display: block;
	float: right; 
}

.center_block {
	display: block;
	width: auto; 
}

