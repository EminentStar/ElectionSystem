<script type="text/javascript">

$(function(){
	search();

});

function search(){
	$.get("<spring:url value="/vote/voteresultlist"/>"
			,function(result){
			
				$(result.pledgeList).each(function(index,pledge){
					var $tr = $("<tr style = 'cursor:pointer;'>");
					$tr.append($("<td>").append(pledge.candi_id));
					$tr.append($("<td>").append(pledge.user_name));
					$tr.append($("<td>").append(pledge.pledge_id));
					$tr.append($("<td>").append(pledge.pledge_contents))
				});
			
			});
}





</script>

