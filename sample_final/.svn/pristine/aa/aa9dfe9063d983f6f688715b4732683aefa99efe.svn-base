	margin-bottom: 10px;
}
.content2_wrap {
	height: 100%;
	width: 100%;
}
#panel {
	display: none;
	float: left;
	padding: 10px;
	position: relative;
	margin-top: 10px;
	margin-bottom: 10px;
	font-size: 12px;
}
.slide {
	border-top-width: 2px;
	border-top-style: solid;
	border-top-color: #96CBCB;
	
	background-image: url(../../img/Javabugs/button.gif);
	clear: both;
	background-repeat: no-repeat;
	background-position: center;
	position: relative;
	float: none;
	width: 100%;
}
.btn-slide {
	text-align: center;
	width: 144px;
	height: 31px;
	display: block;
	color: #fff;
	text-decoration: none;
	background-image: url(../../img/Javabugs/white-arrow.gif); 
	/* ../이렇게되면 상대경로고 /../ 이렇게 하면 절대경로가된다    (보통 배포할때 컨텍스트루트빼고 배포하는경우가 거의 많다?    */
	background-repeat: no-repeat;
	background-position: right -50px;
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
	padding-right: 10px;
	padding-bottom: 0;
	padding-left: 0;
	padding-top: 5px;
	font-family: "留묒� 怨좊뵓";
	font-size: 14px;
	line-height: 100%;
	font-weight: bold;
}
.active {
	background-position: right 12px;
}
