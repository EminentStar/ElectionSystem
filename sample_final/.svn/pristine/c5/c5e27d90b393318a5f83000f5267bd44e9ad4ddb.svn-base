<?xml version="1.0" encoding="UTF-8" ?>  

<!DOCTYPE configuration  
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>

	<typeAliases>
		<!-- parameter Model -->
		<typeAlias alias="JoinParam" type="net.javabugs.web.example.model.dto.JoinParam" />
		<typeAlias alias="PostParam" type="net.javabugs.web.example.model.dto.PostParam" />
		<typeAlias alias="CommentParam" type="net.javabugs.web.example.model.dto.CommentParam" />
		<typeAlias alias="CandiParam" type="net.javabugs.web.example.model.dto.CandiParam"/>
		<typeAlias alias="PledgeParam" type="net.javabugs.web.example.model.dto.PledgeParam"/>
		<typeAlias alias="VoteParam" type="net.javabugs.web.example.model.dto.VoteParam"/>
		

		<!--  result Model -->
		<typeAlias alias="Post" type="net.javabugs.web.example.model.dto.Post" />
		<typeAlias alias="Paging" type="net.javabugs.web.example.model.dto.Paging" />
		<typeAlias alias="Comment" type="net.javabugs.web.example.model.dto.Comment" />
		<typeAlias alias="DeptParam" type="net.javabugs.web.example.model.dto.DeptParam" />
		<typeAlias alias="Candi"  type ="net.javabugs.web.example.model.dto.Candi"/>
		<typeAlias alias="Pledge" type="net.javabugs.web.example.model.dto.Pledge"/>
		<typeAlias alias="GroupMember" type="net.javabugs.web.common.mvc.model.dto.GroupMember" />
		<typeAlias alias="ElectedPledge" type="net.javabugs.web.example.model.dto.ElectedPledge" />
		<typeAlias alias="Vote" type="net.javabugs.web.example.model.dto.Vote" />
		<typeAlias alias="FileInfo" type="net.javabugs.web.example.model.dto.FileInfo" />
		<typeAlias alias="Join" type="net.javabugs.web.example.model.dto.Join" />
		
	</typeAliases>
</configuration>