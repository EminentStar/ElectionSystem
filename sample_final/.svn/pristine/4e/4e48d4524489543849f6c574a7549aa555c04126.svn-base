body {
	width: 245px;
	font: 75%/120% Arial, Helvetica, sans-serif;
	margin-right: 10px;
	margin-left: 10px;
	padding-top: 5;
	padding-right: 0;
	padding-bottom: 0;
	padding-left: 0;
}
a:focus {
	outline: none;
	font-family: "留묒� 怨좊뵓";
}
#panel {
	height: 630px;
	display: none;
	background-color: #FFFFFF;
	float: left;
	padding: 10px;
	position: relative;
}
.slide {
	border-top-width: 2px;
	border-top-style: solid;
	border-top-color: #96CBCB;
	background-image: url(/sample/img/Javabugs/button.gif);
	clear: both;
	background-repeat: no-repeat;
	background-position: center;
	position: relative;
	float: none;
	width: 99%;
	padding-top: 5px;
	font-family: "留묒� 怨좊뵓";
	font-size: 12px;
}
.btn-slide {
	text-align: center;
	width: 144px;
	height: 31px;
	display: block;
	color: #fff;
	text-decoration: none;
	background-image: url(/sample/img/Javabugs/white-arrow.gif);
	background-repeat: no-repeat;
	background-position: right -50px;
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
	padding-right: 10px;
	padding-bottom: 0;
	padding-left: 0;
	padding-top: 5px;
	font-family: "留묒� 怨좊뵓";
	font-size: 13px;
	line-height: 100%;
	font-weight: bold;
}
.active {
	background-position: right 12px;
}
